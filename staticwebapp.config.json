{
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["/images/*.{png,jpg,gif}", "/css/*", "/js/*"]
    },
    "routes": [
      {
        "route": "/*",
        "serve": "/index.html",
        "statusCode": 200
      }
    ],
    "responseOverrides": {
      "404": {
        "rewrite": "/index.html",
        "statusCode": 200
      }
    },
    "mimeTypes": {
      ".json": "text/json"
    },
    "globalHeaders": {
      "cache-control": "no-cache"
    }
  }